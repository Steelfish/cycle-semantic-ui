{
  "name": "cycle-semantic-ui",
  "version": "0.3.4",
  "description": "A set of cyclejs components using Semantic UI",
  "main": "./build/index.js",
  "types": "./build/index.d.ts",
  "scripts": {
    "commit": "git-cz",
    "start": "npm install",
    "serve-docs": "webpack-dev-server --hot --config docs-src/webpack.config.js",
    "build-docs": "rimraf docs/ && webpack --config docs-src/webpack.config.js",
    "build": "rimraf build/ && tsc",
    "test": "mocha test/**/*.ts --compilers ts:ts-node/register",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s"
  },
  "repository": {
    "type": "git",
    "url": "https://www.github.com/Steelfish/cycle-semantic-ui"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "tether": "^1.4.0"
  },
  "peerDependencies": {
    "@cycle/dom": "^16.0.0",
    "@cycle/isolate": "^2.0.0",
    "xstream": "^10.2.0"
  },
  "devDependencies": {
    "@cycle/run": "^3.0.0",
    "@cycle/history": "^5.0.0",
    "@types/es6-shim": "^0.31.32",
    "@types/mocha": "^2.2.39",
    "@types/tether": "^1.4.0",
    "commitizen": "^2.9.6",
    "conventional-changelog-cli": "1.3.1",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.26.1",
    "cz-conventional-changelog": "2.0.0",
    "extract-text-webpack-plugin": "^2.0.0-rc.3",
    "file-loader": "^0.10.0",
    "highlight.js": "^9.9.0",
    "less": "^2.7.2",
    "less-loader": "^2.2.3",
    "mocha": "^3.2.0",
    "rimraf": "^2.6.1",
    "style-loader": "^0.13.1",
    "switch-path": "^1.2.0",
    "ts-loader": "^2.0.1",
    "ts-node": "^2.1.0",
    "typescript": "^2.2.1",
    "typings": "^2.1.0",
    "webpack": "^2.2.1",
    "webpack-append": "^0.1.2",
    "webpack-dev-middleware": "^1.10.1",
    "webpack-dev-server": "^2.4.1",
    "webpack-hot-middleware": "^2.17.1"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
